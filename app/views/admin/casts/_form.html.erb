<%= form_for [:admin, @cast] do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :desc %><br />
    <%= f.text_area :desc %>
  </p>
  <p>
    <%= f.label :demo %><br />
    <%= f.text_field :demo %>
  </p>
  <p><%= f.submit %></p>
<% end %>
